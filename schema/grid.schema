package beta_apart_uranus;

type GridCoordinate {
    // The horizontal offset from the origin (akin to the X coordinate).
    int64 col = 1;

    // The diagonal offset from the origin (akin to the Y coordinate, but at an angle
    // going up and to the right).
    int64 row = 2;
}

component GridPosition {
    id = 199998;
    GridCoordinate pos = 1;
}
